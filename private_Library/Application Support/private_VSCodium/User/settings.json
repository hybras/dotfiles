{
    "window.autoDetectColorScheme": true,
    "workbench.colorTheme": "GitHub Light Default",
    "workbench.preferredDarkColorTheme": "Pitch Black",
    "workbench.preferredLightColorTheme": "GitHub Light Default",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.tree.indent": 12,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
    },
    "workbench.editor.enablePreview": false,
    "workbench.colorCustomizations": {
        "[GitHub Light Default]": {
            // "editorInlayHint.background": "#dfdeded0",
            "editorInlayHint.background": "#ffffff",
            "editorInlayHint.foreground": "#000000d0"
        },
        "[Pitch Black]": {
            "editorInlayHint.background": "#000000",
            "editorInlayHint.foreground": "#00ccff83"
        }
    },
    "editor.fontSize": 16,
    "editor.formatOnSave": true,
    "editor.suggestSelection": "first",
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "editor.wordWrap": "bounded",
    "editor.codeActionsOnSave": {
        "source.fixAll.markdownlint": true,
        "source.fixAll.eslint": true
    },
    "editor.linkedEditing": true,
    "editor.accessibilitySupport": "off",
    "debug.console.fontSize": 16,
    "terminal.external.windowsExec": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.EXE",
    "terminal.integrated.env.osx": {
        "FIG_NEW_SESSION": "1"
    },
    "terminal.external.osxExec": "Warp.app",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 500,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/.hg/store/**": true,
        "/target/**": true
    },
    "files.associations": {
        "*.rmd": "markdown"
    },
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.allowForcePush": true,
    "git.closeDiffOnOperation": true,
    "git.enableCommitSigning": true,
    "git.useEditorAsCommitInput": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.defaultDateShortFormat": "D MMM, YY",
    "gitlens.defaultDateFormat": null,
    "gitlens.gitCommands.skipConfirmations": [
        "fetch:command",
        "switch:command",
        "push:command"
    ],
    "remote.SSH.remotePlatform": {
        "umd": "linux"
    },
    "cSpell.userWords": [
        "Imma",
        "asciicasts",
        "hackathon",
        "lockdown"
    ],
    "python.defaultInterpreterPath": "/opt/homebrew/bin/python3",
    "python.languageServer": "Pylance",
    "python.linting.enabled": true,
    "python.formatting.provider": "black",
    "python.formatting.blackPath": "/opt/homebrew/bin/black",
    "[c]": {
        // "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
    },
    "cmake.configureOnOpen": true,
    "C_Cpp.intelliSenseEngine": "Disabled",
    "clangd.path": "/Users/hybras/Library/Application Support/VSCodium/User/globalStorage/llvm-vs-code-extensions.vscode-clangd/install/12.0.1/clangd_12.0.1/bin/clangd",
    "[markdown]": {
        // "editor.defaultFormatter": "dprint.dprint",
        "editor.formatOnSave": true
    },
    "markdown.preview.fontSize": 16,
    "emojisense.markupCompletionsEnabled": false,
    "markdownlint.config": {
        "no-hard-tabs": false,
        "line-length": false,
        "no-blanks-blockquote": false,
    },
    "markdownExtended.puppeteerExecutable": "C:/Program Files/Google/Chrome/Application/chrome.exe",
    "[javascript]": {
        // "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[json]": {
        // "editor.defaultFormatter": "dprint.dprint"
    },
    "html.format.endWithNewline": true,
    "html.format.indentInnerHtml": true,
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.message.error.show": false,
    "latex-workshop.message.warning.show": false,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "redhat.telemetry.enabled": false,
    "powershell.powerShellDefaultVersion": "Windows PowerShell (x86)",
    "vsicons.dontShowNewVersionMessage": true,
    "r.bracketedPaste": true,
    "r.rpath.windows": "C:\\Users\\varun\\scoop\\shims\\r.exe",
    "r.rterm.windows": "C:\\Users\\varun\\scoop\\apps\\python\\current\\Scripts\\radian.exe",
    "yaml.format.singleQuote": true,
    "yaml.format.enable": true,
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
        // "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "auto-rename-tag.activationOnLanguage": [
        "html",
        "xml",
        "php",
        "js",
    ],
    "rust-analyzer.inlayHints.closureReturnTypeHints.enable": "always",
    "[racket]": {
        "editor.rulers": [
            102
        ],
        "editor.wordSeparators": "()[]{}\",'`;#|",
        "editor.formatOnType": true,
        "editor.formatOnSave": true
    },
    "window.zoomLevel": 1
}